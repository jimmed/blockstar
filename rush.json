{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.30.0",
  "pnpmVersion": "4.14.3",
  "nodeSupportedVersionRange": ">=12.16.1 <13.0.0",
  "ensureConsistentVersions": true,
  "pnpmOptions": {
    "useWorkspaces": true
  },
  "repository": {
