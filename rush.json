  },
  "projects": [
    {
      "packageName": "@blockstar/toolchain",
      "projectFolder": "tools/toolchain",
      "shouldPublish": false
    },
    {
      "packageName": "@blockstar/create-package",
      "projectFolder": "tools/create-package",
      "shouldPublish": false
    },
    {
      "packageName": "@blockstar/desktop",
      "projectFolder": "app/desktop"
    }
  ]
}
